@model IEnumerable<Assign2.Models.Patient>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            Name (in full):
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.fullName(), "Details", new { id = item.Id})
        </td>
    </tr>
}
</table>

